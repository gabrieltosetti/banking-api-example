{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "botman/botman": "v2.7.8",
        "botman/driver-web": "v1.5.3",
        "fideloper/proxy": "4.4.2",
        "fruitcake/laravel-cors": "v3.0.0",
        "guzzlehttp/guzzle": "7.5.0",
        "laravel/framework": "v8.83.27",
        "laravel/tinker": "v2.8.0",
        "nesbot/carbon": "2.65.0",
        "ramsey/collection": "1.3.0",
        "ramsey/uuid" : "4.2.3"
    },
    "require-dev": {
        "facade/ignition": "2.17.6",
        "fakerphp/faker": "v1.21.0",
        "laravel/sail": "v1.18.1",
        "mockery/mockery": "1.5.1",
        "nunomaduro/collision": "v5.11.0",
        "phpunit/phpunit": "9.5.28"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
